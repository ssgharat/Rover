<launch>
<!-- Map server -->
<arg name="map_file" default="$(find Rover)/map/Test_1.yaml"/>
<node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)"/>
<!-- Localization-->
<node pkg="amcl" type="amcl" name="amcl" output="screen">
<remap from="scan" to="/laser/scan"/>
<rosparam file="$(find Rover)/config/amcl.yaml" command="load"/>   ## CHECK if line is required
</node>
<!-- Move base -->
<node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
<rosparam file="$(find Rover)/config/costmap_common_params.yaml" command="load" ns="global_costmap"/>
<rosparam file="$(find Rover)/config/costmap_common_params.yaml" command="load" ns="local_costmap"/>
<rosparam file="$(find Rover)/config/local_costmap_params.yaml" command="load"/>
<rosparam file="$(find Rover)/config/global_costmap_params.yaml" command="load"/>
<rosparam file="$(find Rover)/config/base_local_planner_params.yaml" command="load"/>
<remap from="cmd_vel" to="cmd_vel"/>
<remap from="odom" to="odom"/>
<remap from="scan" to="/laser/scan"/>
<param name="base_global_planner" type="string" value="navfn/NavfnROS"/>
<param name="base_local_planner" value="base_local_planner/TrajectoryPlannerROS"/>
</node>
</launch>
